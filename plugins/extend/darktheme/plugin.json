{
    "$schema": "../../../system/schema/extend.json",
    "name": "AdminDark Theme",
    "description": "Dark administration theme",
    "version": "1.0.0",
    "environment": {
        "system": "^8.0"
    },
    "authors": [
        {"name": "Friends of Sunlight CMS", "url": "https://github.com/friends-of-sunlight-cms/"}
    ],
    "class": "DarkThemePlugin",
    "events.admin": [
        {"event": "admin.init", "method": "onAdminInit"},
        {"event": "admin.style.start", "method": "changes"},
        {"event": "admin.style", "method": "addStyles"}
    ]
}