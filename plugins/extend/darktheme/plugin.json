{
    "$schema": "../../../system/schema/extend.json",
    "name": "DarkTheme",
    "description": "Dark administration theme",
    "version": "0.9.0",
    "environment": {
        "system": "^8.0"
    },
    "url": "https://github.com/friends-of-sunlight-cms/",
    "class": "DarkThemePlugin",
    "events.admin": [
        {"event": "admin.head", "method": "onAdminHead"},
        {"event": "admin.style.start", "method": "changes"},
        {"event": "admin.style", "method": "addStyles"}
    ],
  "scripts":["init.php"]
}